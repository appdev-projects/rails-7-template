<div id="<%= dom_id tester %>">
  <p>
    <strong>Product:</strong>
    <%= tester.product.name %>
  </p>

  <%= image_tag tester.product.image, width: "200", height: "200" %>

  <p>
    <strong>SKU:</strong>
    <%= tester.product.sku %>
  </p>

  <p>
    <strong>Condition:</strong>
    <%= tester.condition %>
  </p>

  <p>
    <strong>Trashed at:</strong>
    <%= tester.trashed_at %>
  </p>




</div>
